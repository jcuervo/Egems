<% leaveTypeTrimmed = @leaveType.replace(/\s/g, "") %>
<div class="accordion-group">
  <div class="accordion-heading">
    <a class="accordion-toggle" href="#<%= leaveTypeTrimmed %>">
      <i class="icon-chevron-down"></i> <%= @leaveType %>
    </a>
  </div>
  <div id="<%= leaveTypeTrimmed %>" class="accordion-body" style="display: none;">
    <div class="accordion-inner">
    </div>
  </div>
</div>
